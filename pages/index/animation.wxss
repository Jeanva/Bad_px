/* pages/index/animation.wxss */
.move{
  background-color:antiquewhite;
  /* display: none; */
  height:100rpx;
  width:100rpx;
  position:relative;
  animation:mymove 5s ease-in-out;
}
@keyframes mymove{
  0% { background-color:red;left:0rpx;}
  25% { background-color:yellow;left:100rpx;top:100rpx}
  50% { background-color:green;top:200rpx;top:200rpx;}
  100% {background-color:blueviolet;top:100rpx;left:100rpx;opacity:0}
}
